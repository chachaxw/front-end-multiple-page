{
  "name": "front-end-multiple-page",
  "version": "1.0.0",
  "description": "Front-end multiple page project by gulp and webpack",
  "main": "gulpfile.js",
  "repository": "https://github.com/chachaxw/front-end-multiple-page.git",
  "author": "Wei Zhou <867571123@qq.com>",
  "license": "MIT",
  "scripts": {
    "start": "supervisor node server/server.js"
  },
  "paths": {
    "src": {
      "base": "./src/",
      "sass": "./src/sass/",
      "css": "./src/css/",
      "js": "./src/js/",
      "img": "./src/img/",
      "views": "./src/views/"
    },
    "dist": {
      "base": "./dist/",
      "css": "./dist/css/",
      "js": "./dist/js/",
      "img": "./dist/img/",
      "views": "./dist/views/"
    },
    "build": {
      "base": "./build/",
      "css": "./build/css/",
      "js": "./build/js/",
      "img": "./build/img/",
      "views": "./build/views/"
    },
    "favicon": {
      "src": "./src/img/favicon.ico"
    },
    "scss": []
  },
  "globs": {
    "img": [
      "./src/img/**/*.{png,jpeg,gif,svg}"
    ],
    "components": [
      "./src/components/**/*.{vue,jsx}"
    ],
    "sass": [
      "./src/sass/**/*.scss"
    ],
    "js": [
      "./src/js/**/*.js",
      "!./src/js/lib/*.js"
    ],
    "distJs": [
      "./node_modules/vue/dist/vue.min.js",
      "./node_modules/axios/dist/axios.min.js"
    ],
    "views": [
      "./src/views/**/*.html"
    ]
  },
  "devDependencies": {
    "babel-core": "^6.24.0",
    "babel-eslint": "^7.2.0",
    "babel-loader": "^6.4.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-es2015": "^6.24.0",
    "body-parser": "^1.17.1",
    "browser-sync": "^2.18.8",
    "chalk": "^1.1.3",
    "cors": "^2.8.1",
    "critical": "^0.8.4",
    "css-loader": "^0.27.1",
    "del": "^2.2.2",
    "eslint": "^3.17.1",
    "eslint-config-airbnb-base": "^11.1.1",
    "eslint-friendly-formatter": "^2.0.7",
    "eslint-loader": "^1.7.0",
    "eslint-plugin-html": "^2.0.1",
    "eslint-plugin-import": "^2.2.0",
    "express": "^4.15.2",
    "fancy-log": "^1.3.0",
    "file-loader": "^0.10.1",
    "gulp": "^3.9.1",
    "gulp-autoprefixer": "^3.1.1",
    "gulp-babel": "^6.1.2",
    "gulp-cached": "^1.1.1",
    "gulp-changed": "^2.0.0",
    "gulp-clean": "^0.3.2",
    "gulp-concat": "^2.6.1",
    "gulp-css-base64": "^1.3.4",
    "gulp-css-spriter": "^0.4.0",
    "gulp-cssnano": "^2.1.2",
    "gulp-debug": "^3.1.0",
    "gulp-favicons": "^2.2.7",
    "gulp-header": "^1.8.8",
    "gulp-if": "^2.0.2",
    "gulp-imagemin": "^3.1.1",
    "gulp-load-plugins": "^1.5.0",
    "gulp-newer": "^1.3.0",
    "gulp-open": "^2.0.0",
    "gulp-plumber": "^1.1.0",
    "gulp-print": "^2.0.1",
    "gulp-rename": "^1.2.2",
    "gulp-replace": "^0.5.4",
    "gulp-rev": "^7.1.2",
    "gulp-sass": "^3.1.0",
    "gulp-size": "^2.1.0",
    "gulp-sourcemaps": "^2.4.1",
    "gulp-uglify": "^2.1.2",
    "gulp-watch": "^4.3.11",
    "html-webpack-plugin": "^2.28.0",
    "run-sequence": "^1.2.2",
    "style-loader": "^0.16.0",
    "vinyl-named": "^1.1.0",
    "vue-loader": "^11.3.1",
    "vue-template-compiler": "^2.2.4",
    "vue-template-es2015-compiler": "^1.5.1",
    "webpack": "^2.3.1",
    "webpack-dev-server": "^2.4.2",
    "webpack-stream": "^3.2.0"
  },
  "dependencies": {
    "axios": "^0.15.3",
    "vue": "^2.2.2"
  }
}
