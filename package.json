{
  "name": "front-end-multiple-page",
  "version": "1.0.0",
  "description": "Front-end multiple page project",
  "main": "gulpfile.js",
  "repository": "https://github.com/chachaxw/front-end-multiple-page.git",
  "author": "Wei Zhou <867571123@qq.com>",
  "license": "MIT",
  "paths": {
    "src": {
      "base": "./src/",
      "scss": "./src/sass/",
      "css": "./src/css/",
      "js": "./src/js/",
      "img": "./src/img/"
    },
    "dist": {
       "base": "./public/",
       "css": "./public/css/",
       "js": "./public/js/",
       "img": "./public/img/"
     },
    "build": {
      "base": "./build/",
      "css": "./build/css/",
      "js": "./build/js/",
      "views": "./build/views/",
      "img": "./build/img/"
    },
    "scss": []
  },
  "vars": {
    "scssName": "style.scss",
    "cssName": "style.css"
  },
  "globs": {
    "img": [
      "./public/img/"
    ],
    "components": [
      "./src/components/**/*.vue"
    ]
  },
  "devDependencies": {
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-es2015": "^6.22.0",
    "browser-sync": "^2.18.8",
    "chalk": "^1.1.3",
    "critical": "^0.8.4",
    "css-loader": "^0.26.2",
    "eslint": "^3.16.1",
    "fancy-log": "^1.3.0",
    "file-loader": "^0.10.1",
    "gulp": "^3.9.1",
    "gulp-autoprefixer": "^3.1.1",
    "gulp-babel": "^6.1.2",
    "gulp-cached": "^1.1.1",
    "gulp-changed": "^2.0.0",
    "gulp-clean": "^0.3.2",
    "gulp-concat": "^2.6.1",
    "gulp-css-base64": "^1.3.4",
    "gulp-css-spriter": "^0.4.0",
    "gulp-cssnano": "^2.1.2",
    "gulp-debug": "^3.1.0",
    "gulp-favicons": "^2.2.7",
    "gulp-fontello": "^0.4.6",
    "gulp-header": "^1.8.8",
    "gulp-if": "^2.0.2",
    "gulp-imagemin": "^3.1.1",
    "gulp-load-plugins": "^1.5.0",
    "gulp-newer": "^1.3.0",
    "gulp-open": "^2.0.0",
    "gulp-plumber": "^1.1.0",
    "gulp-print": "^2.0.1",
    "gulp-rename": "^1.2.2",
    "gulp-replace": "^0.5.4",
    "gulp-rev": "^7.1.2",
    "gulp-sass": "^3.1.0",
    "gulp-size": "^2.1.0",
    "gulp-sourcemaps": "^2.4.1",
    "gulp-uglify": "^2.0.1",
    "gulp-watch": "^4.3.11",
    "webpack": "^2.2.1",
    "webpack-dev-server": "^2.4.1"
  },
  "dependencies": {
    "vue": "^2.2.1"
  }
}
